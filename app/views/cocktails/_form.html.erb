<%= simple_form_for(@cocktail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs text-center">
    <%= f.input :name %>
    <%= f.input :photo, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.submit "add a cocktail", class: "btn btn-primary" %>
  </div>

<% end %>
